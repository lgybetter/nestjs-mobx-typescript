{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "server": "tsc && node dist/index.js",
    "watch-update": "nodemon --watch '**/*' -e ts,tsx --exec 'ts-node' --project 'tsconfig.server.json' ./server/index.ts",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "koa": "^2.13.0",
    "koa-router": "^9.1.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "next": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.7.3"
  },
  "license": "ISC"
}
